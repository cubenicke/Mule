<Ui xmlns="http://www.blizzard.com/wow/ui/"  
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xsi:schemaLocation="http://www.blizzard.com/wow/ui/ FrameXML\UI.xsd">
	<!-- $Id$ -->
	
	<!-- Localization -->
	
	<!-- My Script -->
	<Script file="Mule.lua"/>

	<!-- My AddOn Frames -->
	<Frame name="MainFrame" parent="UIParent">
		<Scripts>
			<OnLoad>
				Mule_OnLoad();
			</OnLoad>
			<OnUpdate>
				Mule_OnUpdate();
			</OnUpdate>
			<OnEvent>
				Mule_OnEvent(event);
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>